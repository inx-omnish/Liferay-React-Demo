{
    "name": "react-cli-module",
    "version": "1.0.0",
    "description": "React Cli Module",
    "devDependencies": {
        "@babel/cli": "^7.7.5",
        "@babel/core": "^7.7.5",
        "@babel/preset-env": "^7.7.6",
        "@babel/preset-react": "^7.7.4",
        "copy-webpack-plugin": "4.6.0",
        "liferay-npm-build-support": "^2.32.0",
        "liferay-npm-bundler": "^2.32.0",
        "liferay-npm-bundler-loader-css-loader": "^2.32.0",
        "webpack": "4.29.6",
        "webpack-cli": "3.3.0",
        "webpack-dev-server": "3.2.1"
    },
    "dependencies": {
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "scripts": {
        "build": "babel --source-maps -d build src && npm run copy-assets && liferay-npm-bundler",
        "copy-assets": "lnbs-copy-assets",
        "translate": "lnbs-translate",
        "deploy": "npm run build && lnbs-deploy",
        "start": "lnbs-start"
    },
    "portlet": {
        "com.liferay.portlet.display-category": "category.react",
        "com.liferay.portlet.header-portlet-css": "/css/styles.css",
        "com.liferay.portlet.instanceable": true,
        "javax.portlet.name": "react-yeoman-module",
        "javax.portlet.display-name": "React Yeoman Module",
        "javax.portlet.security-role-ref": "power-user,user",
        "javax.portlet.resource-bundle": "content.Language"
    },
    "main": "index.js"
}
